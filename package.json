{
  "name": "tanstack-cacher",
  "version": "1.3.4",
  "description": "A lightweight cache management utility for TanStack Query that simplifies adding, updating, deleting, and synchronizing cached data",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hacagahasanli/tanstack-cacher"
  },
  "author": {
    "name": "Hasanli Hajagha",
    "email": "hacagahasanli@gmail.com",
    "url": "https://github.com/hacagahasanli"
  },
  "maintainers": [
    {
      "name": "Hasanli Hajagha",
      "email": "hacagahasanli@gmail.com",
      "web": "http://github.com/hacagahasanli"
    },
    {
      "name": "Sahira Mammadova",
      "email": "mammadovasahira@gmail.com",
      "web": "https://github.com/SahiraMamedova"
    }
  ],
  "homepage": "https://github.com/hacagahasanli/tanstack-cacher#readme",
  "keywords": [
    "react",
    "react-query",
    "tanstack-query",
    "cache",
    "cache-manager",
    "optimistic-updates"
  ],
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "styles",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "tsup --watch",
    "locale-build": "tsup",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts,.tsx",
    "build": "tsup && npm run copy-dist",
    "test": "echo \"No tests yet\" && exit 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "copy-dist": "cp dist/index.js index.js && cp dist/index.d.ts index.d.ts"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.62.11",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
